		<http:request-connection host="${sf.request_token.host}" protocol="HTTPS"/>
	</http:request-config>
	<http:request-config name="HTTP_Request_configuration1" doc:name="HTTP Request configuration" doc:id="34dd034e-f077-4978-87e9-c04954581229" >
		<http:request-connection protocol="HTTPS" host="${sf.request_data.host}" />
	</http:request-config>
	<api-gateway:autodiscovery apiId="${int196.api.id}" ignoreBasePath="true" doc:name="API Autodiscovery" doc:id="0a639966-df78-4b1b-a13d-eb77fbde46cd" flowRef="implementationFlow" />
